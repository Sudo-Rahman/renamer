{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:allow-join",
    "event:default",
    "window:default",
    "app:default",
    "image:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "fs:allow-stat",
    "fs:allow-read-dir",
    "fs:allow-rename",
    "fs:allow-read-file",
    "fs:allow-read",
    "fs:allow-write",
    "window:allow-start-dragging",
    "dialog:default",
    "dialog:default"
  ]
}