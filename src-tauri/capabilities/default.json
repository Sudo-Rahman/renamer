{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:window:default",
    "core:path:allow-join",
    "core:path:default",
    "core:app:default",
    "core:image:default",
    "core:resources:default",
    "core:menu:default",
    "core:tray:default",
    "shell:allow-open",
    "fs:allow-stat",
    "fs:allow-read-dir",
    "fs:allow-rename",
    "fs:allow-read-file",
    "fs:allow-read",
    "fs:allow-write",
    "core:window:allow-start-dragging",
    "dialog:default",
    "shell:default",
    "os:default",
    "core:event:default",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:window:allow-start-dragging",
    "core:window:allow-toggle-maximize",
    "store:allow-get",
    "store:allow-set",
    "store:allow-save",
    "store:allow-load",
    "updater:default",
    "updater:allow-check",
    "updater:allow-download",
    "updater:allow-install",
    "updater:allow-download-and-install",
    "updater:default",
    "process:default",
    "http:default"
  ]
}